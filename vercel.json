{
 "version": 2,
  "crons": [
   {"path": "/person", "schedule": "0 0 * * *"}
  ],
  "builds": [
   { "src": "src/main.ts", "use": "@vercel/node"}
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [ "GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS" ]
    }
  ]
}